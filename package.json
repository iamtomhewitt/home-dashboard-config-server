{
	"name": "home-dashboard-config-server",
	"version": "0.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"client": "cd client && npm start",
		"lint": "eslint --ignore-path .gitignore . && npm run lint-sass",
		"lint-fix": "eslint --ignore-path .gitignore . --fix",
		"lint-sass": "sass-lint --config .sasslintrc client/src/**/*.scss --verbose --no-exit",
		"release": "github-releaser",
		"server": "cd server && npm start",
		"start": "concurrently \"npm run server\" \"npm run client\"",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/iamtomhewitt/home-dashboard-config-server.git",
		"apiUrl": "https://api.github.com/repos/iamtomhewitt/home-dashboard-config-server"
	},
	"author": "Tom Hewitt",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/iamtomhewitt/home-dashboard-config-server/issues"
	},
	"homepage": "https://github.com/iamtomhewitt/home-dashboard-config-server#readme",
	"dependencies": {
		"concurrently": "^5.3.0",
		"github-releaser": "^1.5.0",
		"prop-types": "^15.7.2"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.4.2",
		"@babel/preset-react": "^7.0.0",
		"babel-eslint": "^10.0.1",
		"@commitlint/cli": "^11.0.0",
		"@commitlint/config-conventional": "^11.0.0",
		"cz-conventional-changelog": "^2.1.0",
		"eslint": "^7.11.0",
		"eslint-config-airbnb": "^18.2.0",
		"eslint-plugin-import": "^2.22.0",
		"eslint-plugin-jsx-a11y": "^6.3.1",
		"eslint-plugin-react": "^7.20.6",
		"eslint-plugin-react-hooks": "^1.7.0",
		"husky": "^1.1.2",
		"validate-commit-msg": "^2.14.0",
		"sass-lint": "^1.13.1"
	},
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
